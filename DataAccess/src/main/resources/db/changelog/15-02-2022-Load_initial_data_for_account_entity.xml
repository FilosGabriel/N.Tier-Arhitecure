<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.7.xsd">


    <changeSet id="15-02-2022-load_data" author="Gabriel Filos">
        <loadData encoding="UTF-8" file="./data/account.csv" relativeToChangelogFile="true" tableName="account"
                  schemaName="financial">
            <column header="id" name="id" type="NUMERIC"/>
            <column header="name" name="name" type="TEXT"/>
            <column header="lastSeen" name="last_seen" type="timestamp"/>
            <column header="note" name="note" type="text"/>
        </loadData>

    </changeSet>

</databaseChangeLog>